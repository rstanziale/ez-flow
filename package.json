{
  "name": "@rs-box/ez-flow",
  "version": "1.1.2",
  "description": "Library for a workflow engine",
  "repository": {
    "type": "git",
    "url": "https://github.com/rstanziale/ez-flow.git"
  },
  "keywords": [
    "workflow",
    "typescript"
  ],
  "scripts": {
    "build": "tsc",
    "coverage": "vitest run --coverage",
